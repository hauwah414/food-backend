{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "aliyuncs/oss-sdk-php": "~2.0",
        "arcanedev/log-viewer": "~4.7.0",
        "aws/aws-sdk-php": "3.0",
        "bogardo/mailgun": "^5.1",
        "brozot/laravel-fcm": "^1.2",
        "barryvdh/laravel-dompdf": "^0.9.0",
        "cmdisp/monolog-microsoft-teams": "^1.2",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "^4.0",
        "fruitcake/laravel-cors": "^1.0",
        "intervention/image": "^2.5",
        "jacobcyl/ali-oss-storage": "2.1",
        "laravel/framework": "5.8.*",
        "laravel/passport": "^7.4",
        "laravel/slack-notification-channel": "^2.0",
        "laravel/socialite": "~4.0",
        "laravel/tinker": "^1.0",
        "lcobucci/jwt": "3.3.3",
        "league/flysystem-aws-s3-v3": "^1.0",
        "maatwebsite/excel": "^3.1",
        "mpociot/laravel-apidoc-generator": "^3.12",
        "ncjoes/office-converter": "^1.0",
        "nwidart/laravel-modules": "^5.0",
        "orangehill/iseed": "^2.6",
        "php-http/guzzle6-adapter": "^1.1",
        "phpoffice/phpword": "^0.18.3",
        "rap2hpoutre/fast-excel": "^2.0",
        "reliese/laravel": "^1.1",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "smartins/passport-multiauth": "^4.1",
        "spatie/laravel-backup": "^6.7",
        "squizlabs/php_codesniffer": "^3.7",
        "stephenlake/php-google-holidays": "^1.0",
        "superbalist/laravel-google-cloud-storage": "^2.2",
        "tecnickcom/tcpdf": "^6.5",
        "xendit/xendit-php": "^2.17"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "xethron/migrations-generator": "^2.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
			"Modules\\": "Modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "php-http/discovery": false
        }
    }
}
